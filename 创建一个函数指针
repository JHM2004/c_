//函数指针：
#include<stdio.h>
int add(int a, int b)
{
	return a + b;
}

int main()
{
	int(*ret)(int, int) = &add;
	int a = (*ret)(1, 2);
	printf("%d", a);
	return 0;
}
//函数名就是函数名的地址
.................................................................................

//类型重定义：（typedef）
             //void(*signal(int, void(*)(int)))(int)
             //    //typedef unsigned int uint
             //    typedef void(*a)(int)
             //    变成了：
             //    a signal(int,a)
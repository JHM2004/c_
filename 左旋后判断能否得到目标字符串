
#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>
#include<string.h>


int string_rotate(char* p, char*p1)
{
	int i = 0;
	int n = strlen(p);
	for (i = 0; i < n; i++)
	{
		//一次左旋：
		char ret = *p;
		int j = 0;
		for (j = 0; j <n - 1; j++)
		{
			*(p + j) = *(p + j + 1);
		}
		*(p + n - 1) = ret;
		if (strcmp(p, p1) == 0)
		{
			return 1;
		}
	}
	return 0;
}

int main()
{
	char arr1[10] = "ABCDEF";
	char arr2[10] = "ABCDEF";
	int ret=string_rotate(arr1,arr2);
	if (ret == 1)
	{
		printf("找到了\n");
	}
	else
	{
		printf("找不到\n");
	}
	return 0;
}
